{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background":{
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "http://*/",
    "https://*/",
    "webRequestBlocking"
  ]
}